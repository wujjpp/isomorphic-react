{
  "name": "isomorphic-react",
  "version": "1.0.0",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./tools/run start",
    "start:dist": "node ./build/server.js",
    "clean": "babel-node ./tools/run clean",
    "build:client": "cross-env NODE_ENV=production babel-node ./tools/run build-client",
    "build:server": "cross-env NODE_ENV=production babel-node ./tools/run build-server",
    "build": "cross-env NODE_ENV=production babel-node ./tools/run build",
    "analyse": "babel-node ./tools/analyse",
    "analyse:client": "webpack-bundle-analyzer build/webpack-client-stats.json build/public",
    "analyse:server": "webpack-bundle-analyzer build/webpack-server-stats.json",
    "test": "mocha \"src/**/*.test.js\" --require babel-register --require test/setup.js",
    "eslint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wujjpp/isomorphic-react.git"
  },
  "keywords": [
    "react",
    "starter kit",
    "universal web",
    "isomorphic web",
    "webpack",
    "express",
    "babel",
    "es6",
    "template engine"
  ],
  "author": "Wu Jian Ping <wujjpp@hotmail.com>",
  "maintainers": [
    "Wu Jian Ping <wujjpp@hotmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wujjpp/isomorphic-react/issues"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "deep-equal": "^1.0.1",
    "express": "^4.14.0",
    "glob": "^7.1.1",
    "helmet": "^3.4.0",
    "lodash": "^4.17.4",
    "object-assign": "^4.1.1",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-dom-stream": "^0.5.1",
    "react-helmet": "^5.0.0-beta",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-side-effect": "^1.1.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.4.11"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.2.2",
    "babel-preset-react": "^6.22.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "browser-sync": "^2.18.7",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.1",
    "easy-pie-chart": "^2.1.7",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-css-modules": "^2.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
